[user]
	name = ohnorobo
	email = [REDACTED]

[color]
	ui = 1
  [color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
  [color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
  [color "status"]
    added = yellow
    changed = green
    untracked = cyan

[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    unfuck = checkout
    df = diff
    lg = log -p
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    stat = status
    cherrypick = cherry-pick
    undo = reset --soft HEAD^

    prune-all = !git remote | xargs -n 1 git remote prune
[help]
	autocorrect = 1
